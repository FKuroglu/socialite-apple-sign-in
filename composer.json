{
  "name": "nerdzlab/laravel-apple-sign-in",
  "description": "Extension of laravel/socialite package. Adds apple provider to sign in.",
  "keywords": [
    "nerdzlab",
    "laravel-apple-sign-in"
  ],
  "homepage": "https://github.com/nerdzlab/laravel-apple-sign-in",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Bohdan Tkachuk",
      "email": "bogdantk1@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "firebase/php-jwt": "^5.2",
    "laravel/socialite": "^4.0",
    "ext-openssl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Nerdzlab\\LaravelAppleSignIn\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nerdzlab\\LaravelAppleSignIn\\LaravelAppleSignInServiceProvider"
      ]
    }
  }
}
